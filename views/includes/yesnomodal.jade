//modal dialogs
#yesNoModal.modal.fade(tabindex='-1',role="dialog",aria-labelledby="myModalLabel",aria-hidden='true')
    .modal-dialog
        .modal-content
            .modal-header
                button.close(type='button', data-dismiss='modal')
                    span(aria-hidden='true') &times;
                    span.sr-only Close
                h4#deleteCompanyLabel.modal-title(data-bind='text:ynModal.title')
            .modal-body
                p(data-bind='text: ynModal.message')
            .modal-footer
                button.btn.btn-default(type='button',data-dismiss='modal', data-bind='text: ynModal.cancelText')
                button.btn.btn-danger(type='button',data-dismiss='modal',data-bind='text: ynModal.actionText, click: ynModal.actionFunc')